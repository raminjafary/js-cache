{
  "name": "psyche",
  "version": "1.0.0",
  "description": "Async Cache, minimal and fast",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "raminjafary <raminjafary1993@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:raminjafary/psyche.git"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "ts-node -P tsconfig.json ./tests/fixtures/index.ts",
    "build": "yarn clean && tsc",
    "test-debug": "node --inspect-brk ./node-modules/jest/bin/jest.js --runInBand --watch",
    "test": "jest --verbose --coverage",
    "lint": "eslint --ext .ts --ignore-path .gitignore .",
    "lint:fix": "yarn lint --fix",
    "setup": "yarn install && yarn lint:fix && jest && yarn build",
    "release": "standard-version $* && git push --follow-tags && npm publish"
  },
  "devDependencies": {
    "@types/jest": "26.0.20",
    "@types/node": "14.14.22",
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "@typescript-eslint/parser": "4.14.1",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "standard-version": "9.1.0",
    "ts-jest": "26.4.4",
    "ts-loader": "8.2.0",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  },
  "keywords": [
    "javascript",
    "typescript",
    "local-storage",
    "session-storage",
    "in-memory-cache",
    "dynamic-cache",
    "real-time-cache",
    "concurrent-cache"
  ],
  "browserslist": "> 0.25%, not dead",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn build"
    }
  },
  "lint-staged": {
    "**/*.{ts, js}": [
      "prettier --write",
      "eslint --fix",
      "jest --findRelatedTests",
      "git add"
    ]
  }
}
