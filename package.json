{
  "name": "raminjafary",
  "version": "1.0.0",
  "description": "A simple fast memory-efficient caching library",
  "main": "src/index.js",
  "repository": "git@github.com:raminjafary/js-cache.git",
  "author": "raminjafary <raminjafary1993@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --mode development",
    "build": "NODE_ENV=production webpack",
    "test": "jest --verbose --coverage",
    "test-debug": "node --inspect-brk ./node-modules/jest/bin/jest.js --runInBand --watch",
    "test:watch": "jest --watch",
    "lint": "./node_modules/.bin/eslint --ext .ts --ignore-path .gitignore .",
    "lint:fix": "yarn lint --fix",
    "setup": "yarn install && yarn lint && jest && yarn build"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/compression-webpack-plugin": "^2.0.1",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.0",
    "@types/webpack": "^4.41.6",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "compression-webpack-plugin": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3"
  },
  "keywords": [
    "javascript",
    "typescript",
    "local-storage",
    "session-storage",
    "in-memory-cache",
    "dynamic-cahce",
    "real-time-cache",
    "concurrent-cache"
  ],
  "browserslist": "> 0.25%, not dead",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn build"
    }
  },
  "lint-staged": {
    "**/*.{ts, js}": [
      "prettier --write",
      "eslint --fix",
      "jest --findRelatedTests",
      "git add"
    ]
  }
}
