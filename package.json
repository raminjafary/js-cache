{
  "name": "psyche",
  "version": "1.0.0",
  "description": "Async Cache, minimal and fast",
  "main": "src/index.js",
  "repository": "git@github.com:raminjafary/psyche.git",
  "author": "raminjafary <raminjafary1993@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "webpack serve --mode development --entry ./playground/index.ts",
    "build": "NODE_ENV=production webpack --progress",
    "test": "jest --verbose --coverage",
    "test-debug": "node --inspect-brk ./node-modules/jest/bin/jest.js --runInBand --watch",
    "test:watch": "jest --watch",
    "lint": "./node_modules/.bin/eslint --ext .ts --ignore-path .gitignore .",
    "lint:fix": "yarn lint --fix",
    "setup": "yarn install && yarn lint && jest && yarn build"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-typescript": "7.12.7",
    "@types/compression-webpack-plugin": "6.0.2",
    "@types/html-webpack-plugin": "3.2.4",
    "@types/jest": "26.0.19",
    "@types/node": "14.14.20",
    "@types/webpack": "4.41.25",
    "@typescript-eslint/eslint-plugin": "4.12.0",
    "@typescript-eslint/parser": "4.12.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "compression-webpack-plugin": "7.1.1",
    "eslint": "7.17.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "html-webpack-plugin": "4.5.1",
    "husky": "4.3.6",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "ts-jest": "26.4.4",
    "ts-loader": "8.0.14",
    "ts-node": "9.1.1",
    "typescript": "4.1.3",
    "webpack": "5.11.1",
    "webpack-cli": "4.3.1",
    "webpack-dev-server": "3.11.1"
  },
  "keywords": [
    "javascript",
    "typescript",
    "local-storage",
    "session-storage",
    "in-memory-cache",
    "dynamic-cache",
    "real-time-cache",
    "concurrent-cache"
  ],
  "browserslist": "> 0.25%, not dead",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn build"
    }
  },
  "lint-staged": {
    "**/*.{ts, js}": [
      "prettier --write",
      "eslint --fix",
      "jest --findRelatedTests",
      "git add"
    ]
  }
}
